<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Include user preferences DataStore -->
    <include domain="file" path="datastore/user_preferences.preferences_pb"/>
    
    <!-- Include any other files in datastore directory -->
    <include domain="file" path="datastore/"/>
    
    <!-- Exclude update APK file from backup -->
    <exclude domain="file" path="update.apk"/>
</full-backup-content>
